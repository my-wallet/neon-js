{
  "name": "@cityofzion/neon-js",
  "description": "Neon-JS SDK for interacting with NEO blockchain",
  "version": "3.7.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfZion/neon-js.git"
  },
  "keywords": [
    "neo",
    "antshares",
    "javascript",
    "libraries"
  ],
  "author": "Yak Jun Xiang <snowypowers@gmail.com> (https://github.com/snowypowers)",
  "license": "MIT",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cross-env NODE_ENV=production webpack --mode production",
    "build:lib":"tsc",
    "build:dist":"cross-env NODE_ENV=development webpack --mode development",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint src/**/*.ts --fix",
    "prepublishOnly": "yarn lint && yarn build",
    "pretty": "prettier"
  },
  "dependencies": {
    "@cityofzion/neon-core": "^3.7.2",
    "@cityofzion/neon-nep5": "^3.7.2"
  },
  "devDependencies": {
    "ts-loader": "^4.3.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "files": [
    "dist/"
  ]
}
